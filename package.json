{
  "name": "@sv443/ts-template",
  "version": "1.0.0",
  "description": "General purpose TypeScript project template for VSC, with eslint and nodemon",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsc && node --enable-source-maps out/src/index.js",
    "build": "tsc",
    "watch": "nodemon -e \"ts,d.ts,.env\" -x \"npm start\"",
    "test": "ts-node src/test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sv443/TS-Template.git"
  },
  "keywords": [
    "typescript",
    "template",
    "project-template"
  ],
  "author": {
    "name": "Sv443",
    "url": "https://github.com/Sv443",
    "email": "contact@sv443.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sv443/TS-Template/issues"
  },
  "homepage": "https://github.com/Sv443/TS-Template#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "kleur": "^4.1.5",
    "svcorelib": "^1.18.1"
  }
}